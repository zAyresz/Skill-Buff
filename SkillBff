auto 1 dontlist | ifnot [$curmsg.isaction && $curmsg.sender == $name]{end} | if [$curmsg.content == 'BuffTEXT']{set $cdbuff '[$time+61]'} | if [$curmsg.content == 'Kekkei Genkai!!']{set $cdbuffvip '[$time+61]'} | if [$curmsg.content == 'Buff']{set $cdbuffvip $cdbuff}

auto 100 listas 'Skills Buff' | setcolor 255 0 100 | if [$cdbuff < $time]{say 'BuffJUTSU'}
